<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:XF.Contatos"
             x:Class="XF.Contatos.MainPage">

    <StackLayout>
        <Label FontSize="20" Text="Phone API"></Label>
        <Button x:Name="btnLigar" Text="Telefone" Clicked="btnLigar_Clicked"></Button>
        <Label FontSize="20" Text="GeoLocation - GPS"></Label>
        <Button x:Name="btnLocation" Text="Coordenada" Clicked="btnLocation_Clicked"></Button>
        <Label FontSize="20" Text="Contatos"></Label>
        <Button x:Name="btnContacts" Text="Contatos" Clicked="btnContacts_Clicked"></Button>
    </StackLayout>

</ContentPage>
